<h1>Initiative Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @initiative_statuses.each do |initiative_status| %>
      <tr>
        <td><%= initiative_status.name %></td>
        <td><%= initiative_status.description %></td>
        <td><%= link_to 'Show', initiative_status %></td>
        <td><%= link_to 'Edit', edit_initiative_status_path(initiative_status) %></td>
        <td><%= link_to 'Destroy', initiative_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Initiative Status', new_initiative_status_path %>
