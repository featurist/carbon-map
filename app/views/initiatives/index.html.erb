<p id="notice"><%= notice %></p>

<h1>Initiatives</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Summary</th>
      <th>Image url</th>
      <th>Anticipated carbon saving</th>
      <th>Locality</th>
      <th>Location</th>
      <th>Alternative solution name</th>
      <th>Lead group</th>
      <th>Contact name</th>
      <th>Contact email</th>
      <th>Contact phone</th>
      <th>Partner groups role</th>
      <th>Status</th>
      <th>Gdpr</th>
      <th>Gdpr email verified</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @initiatives.each do |initiative| %>
      <tr>
        <td><%= initiative.name %></td>
        <td><%= initiative.summary %></td>
        <td><%= initiative.image_url %></td>
        <td><%= initiative.anticipated_carbon_saving %></td>
        <td><%= initiative.locality %></td>
        <td><%= initiative.location %></td>
        <td><%= initiative.alternative_solution_name %></td>
        <td><%= initiative.lead_group_id %></td>
        <td><%= initiative.contact_name %></td>
        <td><%= initiative.contact_email %></td>
        <td><%= initiative.contact_phone %></td>
        <td><%= initiative.partner_groups_role %></td>
        <td><%= initiative.status_id %></td>
        <td><%= initiative.gdpr %></td>
        <td><%= initiative.gdpr_email_verified %></td>
        <td><%= link_to 'Show', initiative %></td>
        <td><%= link_to 'Edit', edit_initiative_path(initiative) %></td>
        <td><%= link_to 'Destroy', initiative, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Initiative', new_initiative_path %>
