<div class="CollectionItem-partial">
  <%= f.form_field :url do %>
    <%= f.label :url %>
    <%= f.text_field :url, placeholder: 'e.g. https://twitter.com/carbon-map, https://facebook.com/carbon-map' %>
  <% end %>
  <%= f.form_field :_destroy, class_names: 'CollectionItem-destroy' do %>
    <%= f.label :_destroy, 'Delete' %>
    <%= f.check_box :_destroy, {}, true, false %>
  <% end %>
</div>
